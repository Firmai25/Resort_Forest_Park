<Window x:Class="Resort_Forest_Park.WIndows.AdministratorWindows.MainAdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Resort_Forest_Park.WIndows.AdministratorWindows"
        mc:Ignorable="d"
        Title="MainAdminWindow" Height="450" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Text="Timer"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   FontSize="30"/>
        <Button Content="Сформировать отчёт"
                Grid.Row="1"/>
        <Button Content="Посмотреть история входа"
                Grid.Row="3"
                Click="HistoryLogin_Click"/>
    </Grid>
</Window>
